---
import BaseLayout from './BaseLayout.astro';
const { frontmatter } = Astro.props;

---
<BaseLayout pageTitle={frontmatter.pageTitle}>
    <h1>{frontmatter.pageTitle}</h1>
    <slot />
    <div class="tags">
        {frontmatter.tags.map((tag) => (
            <p><a class="tag" href={`/tags/${tag}`}>{tag}</a></p>
        ))}
    </div>
</BaseLayout>
